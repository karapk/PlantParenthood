generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Plants {
    id        Int      @id 
    name        String   
    genus       String   
    description String  
    isOutdoor   Boolean
    imgURL String @default("placeholder.jpg")
        
}

