generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model OutdoorPlants {
    id        Int      @id @default(autoincrement())
    name      String   @unique
    genus     String   @unique
    description String    
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}